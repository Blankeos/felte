{
  "name": "felte",
  "version": "0.1.3",
  "description": "A form library for Svelte",
  "browser": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Pablo Berganza <pablo@berganza.dev>",
  "license": "MIT",
  "homepage": "https://felte.berganza.dev",
  "repository": "github:pablo-abc/felte",
  "keywords": [
    "svelte",
    "forms",
    "validation"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "rollup -cw",
    "prepublishOnly": "yarn build",
    "build-docs": "yarn node-sass docs-theme/assets/css/main.sass > docs-theme/assets/css/main.css & yarn typedoc"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.0",
    "svelte": "^3.32.0",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.20",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "svelte": "^3.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "immer": "^8.0.1",
    "lodash": "^4.17.20",
    "rollup-plugin-terser": "^7.0.2"
  }
}
